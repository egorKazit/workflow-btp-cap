{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch Application",
      "request": "launch",
      "mainClass": "com.sap.cap.workflowbtp.Application",
      "projectName": "workflowbtp"
    },
    {
      "name": "Run workflow-java",
      "type": "java",
      "request": "launch",
      "mainClass": "com.sap.cap.workflowbtp.Application",
      "projectName": "workflowbtp",
      "preLaunchTask": "Build workflow-java",
      "cwd": "${workspaceFolder}/workflow-java",
      "envFile": "${workspaceFolder}/workflow-java/.env2",
      "args": [
        "--spring.profiles.active=default"
      ],
      "env": {
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/workflowbtpwithjava/workflow-java\\\",\\\"profileName\\\":\\\"default\\\"}\"}"
      }
    }
  ]
}